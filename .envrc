# Static flox environment - avoids constant reloading
# Instead of 'use flox', we manually set the paths
if [[ -d .flox/run/aarch64-darwin.incus.dev/bin ]]; then
  export PATH=".flox/run/aarch64-darwin.incus.dev/bin:.flox/run/aarch64-darwin.incus.dev/sbin:$PATH"
fi

set -a 
CLUSTER_NAME=bioskop
CLUSTER_SUBNET=1
TSID=$(cat .secrets.d/tsid)
TSKEY=$(cat .secrets.d/tskey)
set +a
