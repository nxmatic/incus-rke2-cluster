apiVersion: kustomize.config.k8s.io/v1beta1
kind: Kustomization
resources:
  - ../catalog

# NOTE: Function config resources (setters.yaml, render-*.yaml, Kptfiles) are automatically
# filtered out by kustomize because they have config.kubernetes.io/local-config: "true"
# annotation. They are needed for kpt fn render pipeline execution but should not
# become cluster resources.
